                <DatePicker fx:id="datepickerDateOfBirth" layoutX="7.0" layoutY="264.0" prefHeight="34.0" prefWidth="275.0" promptText="data of birth">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></DatePicker>
                <ChoiceBox fx:id="choiceboxGender" cache="true" layoutX="7.0" layoutY="308.0" prefHeight="34.0" prefWidth="275.0" />
                <ChoiceBox fx:id="choiceboxCountry" layoutX="7.0" layoutY="352.0" prefHeight="34.0" prefWidth="275.0" />
