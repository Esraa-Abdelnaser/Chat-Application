<AnchorPane id="AnchorPane" prefHeight="505.0" prefWidth="790.0" style="-fx-background-color: #fff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gov.iti.jets.presentation.controllers.SignupController">
    <children>
        <AnchorPane prefHeight="503.0" prefWidth="382.0" style="-fx-background-color: linear-gradient(to bottom left,#141E30,#243B55);" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <ImageView fitHeight="160.0" fitWidth="160.0" layoutX="120.0" layoutY="80.0" opacity="0.89" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/user.png" />
