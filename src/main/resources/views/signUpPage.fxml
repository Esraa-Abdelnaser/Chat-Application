                <Button fx:id="btnSignup" layoutX="145.0" layoutY="396.0" stylesheets="@/css/fullpackstyling.css" mnemonicParsing="false" onAction="#clickBtnSignup" prefHeight="34.0" prefWidth="137.0" text="Sign up">
               <font>
                  <Font name="Lucida Calligraphy Italic" size="12.0" />
               </font></Button>
                <Button fx:id="btnSignin" layoutX="7.0" layoutY="440.0" stylesheets="@/css/fullpackstyling.css" mnemonicParsing="false" onAction="#clickBtnSignin" prefHeight="34.0" prefWidth="275.0" text="Sign in">
