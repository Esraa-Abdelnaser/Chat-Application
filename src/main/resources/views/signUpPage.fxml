                  <Font name="Lucida Calligraphy Italic" size="12.0" />
               </font></PasswordField>
                <PasswordField fx:id="txtConfirmPassword" layoutX="7.0" layoutY="176.0" prefHeight="34.0" prefWidth="275.0" promptText="confirm password">
