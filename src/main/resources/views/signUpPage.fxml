                  <Font name="Lucida Calligraphy Italic" size="12.0" />
               </font></TextField>
                <TextField fx:id="txtEmail" layoutX="7.0" layoutY="88.0" prefHeight="34.0" prefWidth="275.0" promptText="email">
               <font>
                  <Font name="Lucida Calligraphy Italic" size="12.0" />
               </font></TextField>
            </children>
        </Pane>
        <ImageView fx:id="imageviewProfileImage" fitHeight="43.0" fitWidth="70.0" layoutX="394.0" layoutY="14.0" onMouseClicked="#clickImageviewProfileImage" pickOnBounds="true" preserveRatio="true">
